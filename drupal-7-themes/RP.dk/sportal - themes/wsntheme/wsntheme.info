name = WSN
description = Theme base WSM.
core = 7.x
version = "7.4.3"
engine = phptemplate

; Image appears on admin/appearance
screenshot = screenshot.png

; wsntheme stylesheets
stylesheets[all][] = css/styles.css
stylesheets[all][] = int.css

; Modernizr acts as a shim for HTML5 elements for older browsers
; as well as detection for mobile devices.

; wsntheme framework scripts.
;scripts[] = src/js/vendors/syc/bet-frames-external.js
;scripts[] = src/js/vendors/jquery.cookie.js
;scripts[] = src/js/vendors/jquery.colorbox-min.js
;scripts[] = src/js/vendors/scrolltofixed/jquery-scrolltofixed-min.js
scripts[] = src/js/vendors/fixto/fixto.min.js
scripts[] = src/js/vendors/jquery-match-height/jquery.matchHeight.js
scripts[] = src/css/vendors/bootstrap/assets/javascripts/bootstrap.min.js
scripts[] = src/js/app.js




; Regions are output in the page.tpl.php
regions[header]         = Header

; Regions for list pages
regions[header_news_list]         = Header News List
regions[header_tips_list]         = Header Tips List
regions[header_bookmakers_list]   = Header Bookmakers List
regions[header_blog_list]         = Header Blog List



; Regions for content
regions[content_top]        = Content Top
regions[content]        = Content
regions[content_bottom]        = Content Bottom
regions[sidebar]  = Sidebar


; Regions for homepage
regions[home_sidebar_in_wrapper] = Home Sidebar in Wrapper
regions[home_row2_main_content]  = Home Row2 main content
regions[home_last_content]       = Home Last content



; Regions for footer
regions[footer_col1]      = Footer Col 1
regions[footer_col2]      = Footer Col 2
regions[footer_col3]      = Footer Col 3
regions[footer_col4]      = Footer Col 4
regions[copyright_last]   = Copyright last
regions[after_body]       = After Body


; Panel Layouts
plugins[panels][layouts] = layouts

; Theme settings:

; Top Bar.
settings[wsntheme_top_bar_enable] = 1
settings[wsntheme_top_bar_grid] = 0
settings[wsntheme_top_bar_sticky] = 0
settings[wsntheme_top_bar_menu_text] = 'Menu'
settings[wsntheme_top_bar_custom_back_text] = 1
settings[wsntheme_top_bar_back_text] = 'Back'
settings[wsntheme_top_bar_is_hover] = 1
settings[wsntheme_top_bar_scrolltop] = 1

; Tooltips.
settings[wsntheme_tooltip_enable] = 1
settings[wsntheme_tooltip_position] = 'top'
settings[wsntheme_tooltip_mode] = 'text'
settings[wsntheme_tooltip_text] = 'More information?'
settings[wsntheme_tooltip_touch] = 0

; Styles and Scripts.
settings[wsntheme_disable_core_css] = 0

; Misc.
settings[wsntheme_html_tags] = 1
settings[wsntheme_messages_modal] = 0
settings[wsntheme_pager_center] = 1

